<%- include('../partials/header') %>

<h2>It's Go Time</h2>
 <table>
   <thead>
     <tr>
       <th>Title</th>
       <th>Start Date</th>
       <th>End Date</th>
       <th>Description</th>
       <th>Itinerary Items</th>
     </tr>
   </thead>
   <tbody>
      <% itineraries.forEach(function(i) { %>
        <tr>
            <td><%= i.title %></td>
            <td><%= i.startDate %></td>
            <td><%= i.endDate %></td>
            <td><%= i.description %></td>
            <td><%= i.items %></td>
            <td><a href="/itineraries/<%= i._id %>">Itinerary Details</a></td>
          </tr>
      <% }) %> 
   </tbody>
 </table>

<%- include('../partials/footer') %>
